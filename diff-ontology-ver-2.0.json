  },
  "private": true,
  "diff-schema": {
    "features": [
      {
        "name": "Function Word Deletion",
        "mnemonic_code": "FW-DEL",
        "description": "Deletion of function words from the reduced register relative to the canonical.",
        "category": "lexical",
        "parse_types": ["dependency", "constituency"],
        "values": [
          "determiner deletion",
          "auxiliary deletion",
          "preposition deletion",
          "coordinating conjunction deletion",
          "subordinating conjunction deletion",
          "pronoun deletion"
        ],
        "value_mnemonics": {
          "determiner deletion": "DET-DEL",
          "auxiliary deletion": "AUX-DEL",
          "preposition deletion": "ADP-DEL",
          "coordinating conjunction deletion": "CCONJ-DEL",
          "subordinating conjunction deletion": "SCONJ-DEL",
          "pronoun deletion": "PRON-DEL"
        },
        "extra": ["deleted_wordform", "pos", "position"]
      },
      {
        "name": "Function Word Addition",
        "mnemonic_code": "FW-ADD",
        "description": "Addition of function words in canonical relative to headline.",
        "category": "lexical",
        "parse_types": ["dependency", "constituency"],
        "values": [
          "determiner addition",
          "auxiliary addition",
          "preposition addition",
          "coordinating conjunction addition",
          "subordinating conjunction addition",
          "pronoun addition"
        ],
        "value_mnemonics": {
          "determiner addition": "DET-ADD",
          "auxiliary addition": "AUX-ADD",
          "preposition addition": "ADP-ADD",
          "coordinating conjunction addition": "CCONJ-ADD",
          "subordinating conjunction addition": "SCONJ-ADD",
          "pronoun addition": "PRON-ADD"
        },
        "extra": ["added_wordform", "pos", "position"]
      },
      {
        "name": "Content Word Deletion",
        "mnemonic_code": "C-DEL",
        "description": "Deletion of content words such as nouns, main verbs, adjectives, adverbs from reduced register.",
        "category": "lexical",
        "parse_types": ["dependency", "constituency"],
        "values": [
          "noun deletion",
          "verb deletion",
          "adjective deletion",
          "adverb deletion"
        ],
        "value_mnemonics": {
          "noun deletion": "NOUN-DEL",
          "verb deletion": "VERB-DEL",
          "adjective deletion": "ADJ-DEL",
          "adverb deletion": "ADV-DEL"
        },
        "extra": ["deleted_wordform", "pos", "position"]
      },
      {
        "name": "Content Word Addition",
        "mnemonic_code": "C-ADD",
        "description": "Addition of content words in canonical relative to headline.",
        "category": "lexical",
        "parse_types": ["dependency", "constituency"],
        "values": [
          "noun addition",
          "verb addition",
          "adjective addition",
          "adverb addition"
        ],
        "value_mnemonics": {
          "noun addition": "NOUN-ADD",
          "verb addition": "VERB-ADD",
          "adjective addition": "ADJ-ADD",
          "adverb addition": "ADV-ADD"
        },
        "extra": ["added_wordform", "pos", "position"]
      },
      {
        "name": "Part of Speech Change",
        "mnemonic_code": "POS-CHG",
        "description": "Part-of-speech category changes between headline and canonical forms.",
        "category": "lexical",
        "parse_types": ["dependency", "constituency"],
        "values": [
          "noun to verb",
          "verb to noun",
          "adjective to noun",
          "noun to adjective",
          "verb to adjective",
          "adjective to verb"
        ],
        "value_mnemonics": {
          "noun to verb": "N2V",
          "verb to noun": "V2N",
          "adjective to noun": "ADJ2N",
          "noun to adjective": "N2ADJ",
          "verb to adjective": "V2ADJ",
          "adjective to verb": "ADJ2V"
        },
        "extra": ["source_pos", "target_pos", "wordform"]
      },
      {
        "name": "Lemma Change",
        "mnemonic_code": "LEMMA-CHG",
        "description": "Differences in lemma form of aligned tokens.",
        "category": "lexical",
        "parse_types": ["dependency", "constituency"],
        "values": ["any lemma change"],
        "value_mnemonics": {
          "any lemma change": "LEM-CHG"
        },
        "extra": ["source_lemma", "target_lemma", "pos"]
      },
      {
        "name": "Surface Form Change",
        "mnemonic_code": "FORM-CHG",
        "description": "Differences in word form (orthographic/inflectional changes).",
        "category": "lexical",
        "parse_types": ["dependency", "constituency"],
        "values": ["any form change"],
        "value_mnemonics": {
          "any form change": "FORM-CHG"
        },
        "extra": ["source_form", "target_form", "lemma", "pos"]
      },
      {
        "name": "Morphological Feature Change",
        "mnemonic_code": "FEAT-CHG",
        "description": "Differences in morphological features such as tense, number, aspect, voice, mood.",
        "category": "morphological",
        "parse_types": ["dependency"],
        "values": [
          "tense change",
          "number change",
          "aspect change",
          "voice change",
          "mood change"
