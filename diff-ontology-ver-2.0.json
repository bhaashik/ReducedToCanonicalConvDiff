          "aspect change": "ASP-CHG",
          "voice change": "VOICE-CHG",
          "mood change": "MOOD-CHG"
        },
        "extra": ["source_feats", "target_feats"]
      },
      {
        "name": "Dependency Relation Change",
        "mnemonic_code": "DEP-REL-CHG",
        "description": "Changes in dependency label assignment and possibly heads.",
        "category": "syntactic",
        "parse_types": ["dependency"],
        "values": [
          "nsubj to obl",
          "obl to advmod",
          "advmod to obl",
          "obj to nsubj",
          "other"
        ],
        "value_mnemonics": {
          "nsubj to obl": "NSUBJ2OBL",
          "obl to advmod": "OBL2ADVMOD",
          "advmod to obl": "ADVMOD2OBL",
          "obj to nsubj": "OBJ2NSUBJ",
          "other": "DEP-MISC"
        },
        "extra": ["source_deprel", "target_deprel", "head_word"]
      },
      {
        "name": "Token Reordering",
        "mnemonic_code": "TOKEN-REORDER",
        "description": "Changes in linear token sequence between headline and canonical.",
        "category": "word-order",
        "parse_types": ["dependency"],
        "values": [
          "fronting",
          "postposing",
          "local swap",
          "long-distance move"
        ],
        "value_mnemonics": {
          "fronting": "FRONT",
          "postposing": "POST",
          "local swap": "LSWAP",
          "long-distance move": "LD-MOVE"
        },
        "extra": ["old_position", "new_position", "wordform"]
      },
      {
        "name": "Constituent Removal",
        "mnemonic_code": "CONST-REM",
        "description": "Removal of constituents from reduced register.",
        "category": "syntactic",
        "parse_types": ["constituency"],
        "values": [
          "NP removal",
          "PP removal",
          "SBAR removal",
          "ADJP removal",
          "VP removal",
          "CP removal"
        ],
        "value_mnemonics": {
          "NP removal": "NP-REM",
          "PP removal": "PP-REM",
          "SBAR removal": "SBAR-REM",
          "ADJP removal": "ADJP-REM",
          "VP removal": "VP-REM",
          "CP removal": "CP-REM"
        },
        "extra": ["removed_label", "removed_words", "start_end_span"]
      },
      {
        "name": "Constituent Addition",
        "mnemonic_code": "CONST-ADD",
        "description": "Addition of constituents in canonical relative to headline.",
        "category": "syntactic",
        "parse_types": ["constituency"],
        "values": [
          "NP addition",
          "PP addition",
          "SBAR addition",
          "ADJP addition",
          "VP addition",
          "CP addition"
        ],
        "value_mnemonics": {
          "NP addition": "NP-ADD",
          "PP addition": "PP-ADD",
          "SBAR addition": "SBAR-ADD",
          "ADJP addition": "ADJP-ADD",
          "VP addition": "VP-ADD",
          "CP addition": "CP-ADD"
        },
        "extra": ["added_label", "added_words", "start_end_span"]
      },
      {
        "name": "Tree Edit Distance",
        "mnemonic_code": "TED",
        "description": "Tree edit distance between constituency parses of headline and canonical.",
        "category": "structural",
        "parse_types": ["constituency"],
        "values": ["numeric"],
        "value_mnemonics": {
          "numeric": "TED"
        },
        "extra": ["operation_list"]
      },
      {
        "name": "Sentence Length Change",
        "mnemonic_code": "LENGTH-CHG",
        "description": "Difference in total tokens or constituents between forms.",
        "category": "statistical",
        "parse_types": ["dependency", "constituency"],
        "values": ["numeric"],
        "value_mnemonics": {
          "numeric": "LEN-DIFF"
        },
        "extra": ["headline_length", "canonical_length"]
      }
    ]
  }
}
