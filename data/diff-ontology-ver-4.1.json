{
  "name": "reducedtocanonicalconvdiff",
  "version": "4.0.0",
  "description": "Enriched schema with comprehensive morphological features from Stanza CoNLL-U parses",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bhaashik/ReducedToCanonicalConvDiff.git"
  },
  "private": true,
  "changelog": {
    "4.0.0": "Enriched FEAT-CHG with all morphological features from Stanza: Person, Gender, Definite, PronType, Poss, NumType, NumForm, Polarity, Reflex, Abbr, ExtPos, Foreign. This ensures complete morphosyntactic coverage."
  },
  "diff-schema": {
