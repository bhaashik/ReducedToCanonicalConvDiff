        "description": "Orthographic or inflectional surface form differences.",
        "category": "lexical",
        "parse_types": ["dependency", "constituency"],
        "values": ["any form change"],
        "value_mnemonics": {
          "any form change": "FORM-CHG"
        },
        "extra": ["source_form", "target_form", "lemma", "pos"]
      },
      {
        "name": "Morphological Feature Change",
        "mnemonic_code": "FEAT-CHG",
        "group": "morphological",
        "description": "Morphological feature differences extracted from Universal Dependencies Stanza parses. Includes all granular morphological features: Tense, Number, Aspect, Voice, Mood, Case, Degree, Person, Gender, Definiteness, PronType, Possessive, NumType, NumForm, Polarity, Reflexive, VerbForm, Abbreviation, ExtPos, Foreign.",
        "category": "morphological",
        "parse_types": ["dependency"],
        "values": [
          "tense change",
          "number change",
          "aspect change",
          "voice change",
          "mood change",
          "case change",
          "degree change",
          "person change",
          "gender change",
          "definiteness change",
          "pronoun type change",
          "possessive change",
          "number type change",
          "number form change",
          "polarity change",
          "reflexive change",
          "verbform change",
          "abbreviation change",
          "external pos change",
          "foreign change"
        ],
        "value_mnemonics": {
          "tense change": "TENSE-CHG",
          "number change": "NUM-CHG",
          "aspect change": "ASP-CHG",
          "voice change": "VOICE-CHG",
          "mood change": "MOOD-CHG",
          "case change": "CASE-CHG",
          "degree change": "DEG-CHG",
          "person change": "PERSON-CHG",
          "gender change": "GENDER-CHG",
          "definiteness change": "DEF-CHG",
          "pronoun type change": "PRONTYPE-CHG",
