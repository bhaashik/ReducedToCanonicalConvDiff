        "parse_types": ["dependency", "constituency"],
        "values": [
          "article deletion",
          "quantifier deletion",
          "auxiliary deletion",
          "preposition deletion",
          "coordinating conjunction deletion",
          "subordinating conjunction deletion",
          "personal pronoun deletion",
          "relative pronoun deletion",
          "demonstrative pronoun deletion"
        ],
        "value_mnemonics": {
          "article deletion": "ART-DEL",
          "quantifier deletion": "QUANT-DEL",
          "auxiliary deletion": "AUX-DEL",
          "preposition deletion": "ADP-DEL",
          "coordinating conjunction deletion": "CCONJ-DEL",
          "subordinating conjunction deletion": "SCONJ-DEL",
          "personal pronoun deletion": "PRON-PERS-DEL",
          "relative pronoun deletion": "PRON-REL-DEL",
          "demonstrative pronoun deletion": "PRON-DEM-DEL"
        },
        "extra": ["deleted_wordform", "pos", "position"]
      },
      {
        "name": "Function Word Addition",
        "mnemonic_code": "FW-ADD",
        "group": "lexical/function",
        "description": "Addition of function words in canonical relative to headline.",
        "category": "lexical",
        "parse_types": ["dependency", "constituency"],
        "values": [
          "article addition",
          "quantifier addition",
          "auxiliary addition",
          "preposition addition",
          "coordinating conjunction addition",
          "subordinating conjunction addition",
          "personal pronoun addition",
          "relative pronoun addition",
          "demonstrative pronoun addition"
        ],
        "value_mnemonics": {
          "article addition": "ART-ADD",
          "quantifier addition": "QUANT-ADD",
          "auxiliary addition": "AUX-ADD",
          "preposition addition": "ADP-ADD",
          "coordinating conjunction addition": "CCONJ-ADD",
          "subordinating conjunction addition": "SCONJ-ADD",
          "personal pronoun addition": "PRON-PERS-ADD",
          "relative pronoun addition": "PRON-REL-ADD",
          "demonstrative pronoun addition": "PRON-DEM-ADD"
        },
        "extra": ["added_wordform", "pos", "position"]
      },
      {
        "name": "Content Word Deletion",
        "mnemonic_code": "C-DEL",
        "group": "lexical/content",
        "description": "Deletion of content words (nouns, verbs, adjectives, adverbs) in reduced register.",
        "category": "lexical",
        "parse_types": ["dependency", "constituency"],
        "values": ["noun deletion", "verb deletion", "adjective deletion", "adverb deletion"],
        "value_mnemonics": {
          "noun deletion": "NOUN-DEL",
          "verb deletion": "VERB-DEL",
          "adjective deletion": "ADJ-DEL",
          "adverb deletion": "ADV-DEL"
        },
        "extra": ["deleted_wordform", "pos", "position"]
      },
      {
        "name": "Content Word Addition",
        "mnemonic_code": "C-ADD",
        "group": "lexical/content",
        "description": "Addition of content words in canonical form.",
        "category": "lexical",
        "parse_types": ["dependency", "constituency"],
        "values": ["noun addition", "verb addition", "adjective addition", "adverb addition"],
        "value_mnemonics": {
          "noun addition": "NOUN-ADD",
          "verb addition": "VERB-ADD",
          "adjective addition": "ADJ-ADD",
          "adverb addition": "ADV-ADD"
        },
        "extra": ["added_wordform", "pos", "position"]
      },
      {
        "name": "Part of Speech Change",
        "mnemonic_code": "POS-CHG",
        "group": "lexical",
        "description": "Part-of-speech changes between aligned tokens.",
        "category": "lexical",
        "parse_types": ["dependency", "constituency"],
        "values": [
          "noun to verb",
          "verb to noun",
          "adjective to noun",
          "noun to adjective",
          "verb to adjective",
          "adjective to verb",
          "finite verb to nonfinite verb",
          "proper noun to common noun",
          "common noun to proper noun"
        ],
        "value_mnemonics": {
          "noun to verb": "N2V",
          "verb to noun": "V2N",
          "adjective to noun": "ADJ2N",
          "noun to adjective": "N2ADJ",
          "verb to adjective": "V2ADJ",
          "adjective to verb": "ADJ2V",
          "finite verb to nonfinite verb": "VFIN2VNFIN",
          "proper noun to common noun": "PROPN2N",
          "common noun to proper noun": "N2PROPN"
        },
        "extra": ["source_pos", "target_pos", "wordform"]
      },
      {
        "name": "Lemma Change",
        "mnemonic_code": "LEMMA-CHG",
        "group": "lexical",
        "description": "Lemma differences in aligned tokens.",
        "category": "lexical",
        "parse_types": ["dependency", "constituency"],
        "values": ["any lemma change"],
        "value_mnemonics": {
          "any lemma change": "LEM-CHG"
        },
        "extra": ["source_lemma", "target_lemma", "pos"]
      },
      {
        "name": "Surface Form Change",
        "mnemonic_code": "FORM-CHG",
