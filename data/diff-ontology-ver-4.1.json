        "extra": ["source_feats", "target_feats", "feature_name", "source_value", "target_value"],
        "feature_definitions": {
          "Tense": "Temporal reference of verb: Past, Pres, Fut",
          "Number": "Grammatical number: Sing, Plur",
          "Aspect": "Viewpoint aspect: Imp, Perf, Prog",
          "Voice": "Grammatical voice: Act, Pass, Mid",
          "Mood": "Grammatical mood: Ind, Imp, Sub, Cnd",
          "Case": "Grammatical case: Nom, Acc, Gen, Dat, etc.",
          "Degree": "Degree of comparison: Pos, Cmp, Sup",
          "Person": "Grammatical person: 1, 2, 3",
          "Gender": "Grammatical gender: Masc, Fem, Neut",
          "Definite": "Definiteness: Def, Ind",
          "PronType": "Pronoun type: Art, Dem, Ind, Int, Prs, Rel",
          "Poss": "Possessive: Yes",
          "NumType": "Numeral type: Card, Ord, Frac, Mult",
          "NumForm": "Numeral form: Word, Digit, Roman",
          "Polarity": "Polarity/Negation: Pos, Neg",
          "Reflex": "Reflexive: Yes",
          "VerbForm": "Verb form: Fin, Inf, Part, Ger, Sup, Conv",
          "Abbr": "Abbreviation: Yes",
          "ExtPos": "External POS (when word functions as different POS)",
          "Foreign": "Foreign word: Yes"
        }
      },
      {
        "name": "Dependency Relation Change",
        "mnemonic_code": "DEP-REL-CHG",
        "group": "syntactic/dependency",
        "description": "Change in dependency relation labels (and possibly heads).",
        "category": "syntactic",
        "parse_types": ["dependency"],
        "values": [
          "nsubj to obl",
          "obl to advmod",
          "advmod to obl",
          "obj to nsubj",
          "nsubj to obj",
          "modifier shift",
          "complement drop",
          "other"
        ],
        "value_mnemonics": {
          "nsubj to obl": "NSUBJ2OBL",
          "obl to advmod": "OBL2ADVMOD",
          "advmod to obl": "ADVMOD2OBL",
          "obj to nsubj": "OBJ2NSUBJ",
          "nsubj to obj": "NSUBJ2OBJ",
          "modifier shift": "MOD-SHIFT",
          "complement drop": "COMP-DROP",
          "other": "DEP-MISC"
        },
        "extra": ["source_deprel", "target_deprel", "head_word"]
